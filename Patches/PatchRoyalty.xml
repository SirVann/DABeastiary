<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Royalty</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>
			
		       <li Class="PatchOperationAdd">
			      <xpath>Defs/TraderKindDef[defName = "Empire_Caravan_TributeCollector"]/stockGenerators</xpath>
                  <value>		
	                     <li Class="StockGenerator_BuySingleDef">
                             <thingDef>DA_Goldilox</thingDef>
                         </li>
                  </value>
				</li>
				
			
		       <li Class="PatchOperationAdd">
			      <xpath>Defs/TraderKindDef[defName = "Empire_Caravan_TributeCollector"]/stockGenerators</xpath>
                  <value>		
	                     <li Class="StockGenerator_BuySingleDef">
                             <thingDef>DA_ImperialRedhound</thingDef>
                         </li>
                  </value>
				</li>
				
		       <li Class="PatchOperationAdd">
			      <xpath>Defs/TraderKindDef[defName = "Empire_Caravan_TributeCollector"]/stockGenerators</xpath>
                  <value>		
	                     <li Class="StockGenerator_BuySingleDef">
                             <thingDef>DA_WoolGoldilox</thingDef>
                         </li>
                  </value>
				</li>
			</operations>
		</match>
	</Operation>
	
</Patch>